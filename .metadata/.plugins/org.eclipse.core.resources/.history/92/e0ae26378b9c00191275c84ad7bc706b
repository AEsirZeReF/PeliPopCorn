package Class;

public class ListaTicket {
	private String nombrePelicula;
	private int idSala;
	private String tipoSala;
	private String horaPro;
	private String cliente;
	private double precio;
	private int cantidad;
	private double subtotal;
	private String fecha;
	
	public ListaTicket(){
		
	}
	public ListaTicket(String nombrePelicula,int idSala,String tipoSala,String horaPro,
			String cliente,double precio,int cantidad,double subtotal,String fecha){
		this.nombrePelicula = nombrePelicula;
		this.idSala = idSala;
		this.tipoSala = tipoSala;
		this.horaPro = horaPro;
		this.cliente = cliente;
		this.precio = precio;
		this.cantidad = cantidad;
		this.subtotal = subtotal;
		this.fecha = fecha;
		
	}
}
